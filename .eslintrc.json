{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": 0,
        "eol-last": 0,
        "indent": "off",
        "no-trailing-spaces": [2, { "skipBlankLines": true }],
        "no-undef": "off",
        "no-underscore-dangle": 0
    },
    "plugins": ["jasmine"]
}
